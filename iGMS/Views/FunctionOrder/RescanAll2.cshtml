                    getUpcTrue(arrayGeneral, arrayData)
                    var idReports = localStorage.getItem('id')
                    var Ctn = JSON.stringify(arrayData)
                    var epcToUpc = JSON.stringify(arrayReportsEpcToUpc)
                    var info = JSON.stringify(arrayInfo)
                    $.ajax({
                        url: '/Home/EditAll',
