            var tbdInfoPreAsn = ""
            $.each(arrayInfoPreASN, function (k, v) {
                tbdInfoPreAsn += `<tr>
                                <td>${v.so}</td>
                                <td>${v.po}</td>
                                <td>${v.sku}</td>
                                <td>${v.ctn.length}</td>
                                <td>${v.qty}</td></tr>
    `
